<resources>
    <style name="Theme.Android" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="color_primary">@color/light_blue</item>
        <item name="color_secondary">@color/white</item>
        <item name="color_tertiary">@color/signal_black</item>
        <item name="color_quaternary">@color/orange</item>
        <item name="color_divider">@color/grey</item>

        <item name="background_color_primary">@color/white</item>
        <item name="background_color_secondary">@color/smoky_white</item>
        <item name="background_color_tertiary">@color/light_blue</item>

        <item name="text_color_primary">@color/white</item>
        <item name="text_color_secondary">@color/signal_black</item>
        <item name="text_color_tertiary">@color/light_blue</item>
        <item name="text_color_quaternary">@color/orange</item>
        <item name="text_color_sevenfold">@color/dark_grey</item>

        <item name="icon_tint_color_primary">@color/light_blue</item>
        <item name="icon_tint_color_secondary">@color/signal_black</item>
        <item name="icon_tint_color_tertiary">@color/orange</item>
    </style>

    <style name="Toolbar" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:textSize" >26sp</item>
        <item name="android:fontFamily">@font/ubuntu_bold</item>
        <item name="android:letterSpacing">.0</item>
    </style>

    <style name="SectionName" parent="ThemeOverlay.AppCompat.Light" >
        <item name="android:textSize" >20sp</item>
        <item name="android:fontFamily">@font/ubuntu_medium</item>
        <item name="android:letterSpacing">.0</item>
    </style>

    <style name="HintsText" parent="ThemeOverlay.AppCompat.Light" >
        <item name="android:textSize" >16sp</item>
        <item name="android:fontFamily">@font/ubuntu_regular</item>
        <item name="android:letterSpacing">.0</item>
    </style>

    <style name="PrimaryText" parent="ThemeOverlay.AppCompat.Light" >
        <item name="android:textSize" >18sp</item>
        <item name="android:fontFamily">@font/ubuntu_regular</item>
        <item name="android:letterSpacing">.0</item>
    </style>

    <style name="NameText" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:textSize" >22sp</item>
        <item name="android:fontFamily">@font/ubuntu_bold</item>
        <item name="android:letterSpacing">.0</item>
    </style>

    <style name="PrimarySpinnerButtonTheme" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:background">?attr/background_color_primary</item>
        <item name="android:src">@drawable/ic_select_btn</item>
        <item name="tint">?attr/color_primary</item>
    </style>

    <style name="PrimaryEditTextHintTheme" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:fontFamily">@font/ubuntu_regular</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">?attr/text_color_tertiary</item>
        <item name="android:letterSpacing">.0</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="PrimaryEditTextTheme" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:fontFamily">@font/ubuntu_regular</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">?attr/text_color_secondary</item>
        <item name="android:letterSpacing">.0</item>
        <item name="android:autofillHints" />
        <item name="android:background">@drawable/bg_edit_text</item>
    </style>

    <style name="SecondaryEditTextTheme" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:fontFamily">@font/ubuntu_regular</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">?attr/text_color_secondary</item>
        <item name="android:letterSpacing">.0</item>
        <item name="android:maxLines" >1</item>
        <item name="android:background">@drawable/bg_edit_text</item>
        <item name="android:completionThreshold">1</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="PrimarySpinnerTheme" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:fontFamily">@font/ubuntu_regular</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">?attr/text_color_secondary</item>
        <item name="android:letterSpacing">.0</item>
        <item name="android:background">@drawable/bg_spinner</item>
    </style>

    <style name="EditProfileSectionLabelTheme" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:fontFamily">@font/ubuntu_medium</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">?attr/text_color_secondary</item>
        <item name="android:letterSpacing">.0</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
    </style>

    <style name="EditProfileHideShowSectionTheme" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:background">?attr/color_secondary</item>
        <item name="android:src">@drawable/ic_select_btn</item>
    </style>

    <style name="PrimaryButtonTheme" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:fontFamily">@font/ubuntu_medium</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">?attr/text_color_primary</item>
        <item name="android:letterSpacing">.0</item>
        <item name="android:textAllCaps">false</item>
        <item name="backgroundTint">?attr/color_primary</item>
        <item name="android:background">@drawable/bg_save_all_btn</item>
    </style>

    <style name="PrimaryTextButtonTheme" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:fontFamily">@font/ubuntu_medium</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">?attr/text_color_primary</item>
        <item name="android:letterSpacing">.0</item>
        <item name="android:textAllCaps">false</item>
        <item name="backgroundTint">?attr/color_primary</item>
        <item name="android:background">@drawable/bg_save_all_btn</item>
    </style>

    <style name="SecondaryToolbarTheme" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:fontFamily">@font/ubuntu_bold</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">?attr/text_color_secondary</item>
        <item name="android:letterSpacing">.0</item>
    </style>

    <style name="PrimaryToolbarTheme" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:fontFamily">@font/ubuntu_bold</item>
        <item name="android:textSize">22sp</item>
        <item name="android:textColor">?attr/text_color_tertiary</item>
        <item name="android:letterSpacing">.0</item>
        <item name="android:tint">?attr/color_primary</item>
    </style>

    <style name="DataContainerTheme" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:fontFamily">@font/ubuntu_bold</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#FF896E</item>
        <item name="android:letterSpacing">.0</item>
        <item name="android:tint">#FF896E</item>
    </style>
</resources>

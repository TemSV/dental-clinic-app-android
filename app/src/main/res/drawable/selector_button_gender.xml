<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <corners android:radius="61dp" />
            <stroke android:width="1dp" android:color="?attr/background_color_primary" />
            <solid android:color="?attr/background_color_tertiary" />
            <size
                android:width="146dp"
                android:height="31dp" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="61dp" />
            <solid android:color="?attr/background_color_primary" />
            <size
                android:width="146dp"
                android:height="31dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="61dp" />
            <solid android:color="?attr/background_color_quaternary" />
            <size
                android:width="146dp"
                android:height="31dp" />
        </shape>
    </item>
</selector>
